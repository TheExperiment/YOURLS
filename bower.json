{
  "name": "YOURLS",
  "private": true,
  "dependencies": {
    "jquery": "*",
    "bootstrap": "*",
    "font-awesome": "*",
    "flag-sprites": "*",
    "bootstrap-social": "*",
    "zeroclipboard": "*"
  },
  "exportsOverride": {
    "jquery": {
      "js": "dist/jquery.min.js"
    },
    "bootstrap": {
      "less": "less/*.less",
      "js": "dist/js/*.min.js"
    },
    "font-awesome": {
      "less": "less/*.less",
      "fonts": "fonts/fontawesome-webfont.*"
    },
    "flag-sprites": {
      "less": "less/*.less",
      "img": "dist/img/*"
    },
    "bootstrap-social": {
      "less": "*.less"
    },
    "zeroclipboard": {
      "js": ["ZeroClipboard.min.js", "ZeroClipboard.swf"]
    }
  }
}
