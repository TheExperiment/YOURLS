{
  "name": "YOURLS",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.2.0",
    "bootstrap-social": "^4.5.0",
    "flag-sprites": "^1.1.0",
    "fontawesome": "^4.1.0",
    "jquery": "^2.1.0",
    "jquery-cookie": "^1.4.0",
    "zeroclipboard": "^2.1.0"
  },
  "exportsOverride": {
    "bootstrap": {
      "less": "less/**/*.less",
      "js": "dist/js/**/*.min.js"
    },
    "bootstrap-social": {
      "less": "*.less"
    },
    "flag-sprites": {
      "less": "less/*.less",
      "img": "dist/img/**/*"
    },
    "fontawesome": {
      "less": "less/**/*.less",
      "fonts": "fonts/fontawesome-webfont.*"
    },
    "jquery": {
      "js": "dist/jquery.min.js"
    },
    "jquery-cookie": {
      "js": "jquery.cookie.js"
    },
    "zeroclipboard": {
      "js": [
        "ZeroClipboard.min.js",
        "ZeroClipboard.swf"
      ]
    }
  }
}
